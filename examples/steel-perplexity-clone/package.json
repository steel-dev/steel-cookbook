    "openai": "^4.63.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "tsx": "^4.16.2",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "fastify",
    "typescript",
    "openai",
    "websearch",
    "steel.dev",
    "scrape",
    "api"
  ]
